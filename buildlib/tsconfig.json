{
  "compilerOptions": {
      "module": "commonjs",
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": false,
      "baseUrl": "lib",
      "target": "ES6",
      "outDir": "dist",
      "declaration": false,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "lib": [
          "es5", "es6"
      ]
  },
  "include": [
      "./*.ts"
  ],
  "exclude": [
      "node_modules"
  ]
}
